                THEN
                  BEGIN
                    MAKE_AND_OUT_GROUP;
                    INIT_GROUP;
                  END;
            END
          ELSE  (*  *)
            BEGIN
              L := LENGTH (LINE);
              P := POS (BL2, LINE);
