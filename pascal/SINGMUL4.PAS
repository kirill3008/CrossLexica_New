                 INC (AMTTI [SOURCE MOD {128}180]);
                END;
            3 : BEGIN
                 INC (CNT3);
                 INC (AMTPR [SOURCE MOD {128}180]);
                END;
          END;
          PRINT_TREE (RIGHT);
        END;
  END;


