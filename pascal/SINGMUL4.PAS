  FUNCTION LOW_CASE (INPSTR : STR23) : STR23;
         (*========*)
    VAR
      I : BYTE;
      OUTSTR : STR23;
  BEGIN
    OUTSTR := INPSTR;
    FOR I := 1 TO LENGTH (INPSTR) DO
      IF INPSTR [I] < ''
        THEN OUTSTR [I] := CHR (ORD (INPSTR [I]) + 32)
        ELSE OUTSTR [I] := CHR (ORD (INPSTR [I]) + 80);
    LOW_CASE := OUTSTR;
  END;


