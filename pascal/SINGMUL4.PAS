  PROCEDURE ALLOC_IN_TREE (TOKEN : STR47;
          (*=============*)FROM : INTEGER;
                           VAR CURROOT : REF);
    VAR
      NEWNODE : REF;
  BEGIN
    IF CURROOT = NIL
      THEN
        BEGIN
          NEW (NEWNODE);
          WITH NEWNODE^ DO
            BEGIN
              KEY := TOKEN;
              SOURCE := FROM;
              LEFT := NIL;
              RIGHT := NIL;
            END;
          CURROOT := NEWNODE;
        END
      ELSE WITH CURROOT^ DO
        IF TOKEN <> KEY
          THEN
            IF TOKEN < KEY
              THEN ALLOC_IN_TREE (TOKEN, FROM, LEFT)
              ELSE ALLOC_IN_TREE (TOKEN, FROM, RIGHT);
  END;


  PROCEDURE PRINT_TREE (T : REF);
          (*==========*)
