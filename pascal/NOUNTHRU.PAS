END;

  { Initialization of both outer arrays }  
PROCEDURE GLOBAL_INIT;
        {============}

{ 1st stage: Initialization of basic nouns array }
PROCEDURE FILL_BASE_NOUNS;
        (*===============*)
VAR
  ERC1, ERC2 : INTEGER;
  I : LONGINT;
  LINE : STR115;
  INP : TEXT;
BEGIN
  ASSIGN (INP, 'BASICS.LST');
  RESET (INP);
  I := 0;
  REPEAT
    READLN (INP, LINE);
    INC(I);
    VAL (COPY(LINE,2,4), BSNCLS [I,1], ERC1);
    VAL (COPY(LINE,6,4), BSNCLS [I,2], ERC2);
    IF ERC1 + ERC2 > 0
      THEN WRITELN ('Conversion error at line ', I, ' of BASICS.LST');
    BSNPAR [I,1] := ORD(LINE [11]) - ORD('0');
    BSNPAR [I,2] := ORD(LINE [13]) - ORD('0');
    BSNSTR [I] := COPY (LINE, 15, LENGTH(LINE)-14);
  UNTIL EOF(INP) OR (I = MAXBSN);
  BN_REACHED := I;
  CLOSE (INP);
  writeln ('BASICS  are  ', BN_REACHED);
END;

{ 2nd stage: Initialization of very long nouns array }
PROCEDURE VLN_ARRAY_INIT;
         {==============}
VAR
  I : LONGINT;
  LINE : STR115;
  XL_DAS : STRING [255];
  INP : TEXT;
BEGIN
 { Form reference file name }
  XL_DAS := GetEnv ('XL_DAS_PATH');
  IF XL_DAS = '' 
    THEN XL_DAS := '../DAS/';
 { Initiate array }
  ASSIGN (INP, XL_DAS+'TOOLONG.DAS');
  RESET (INP);
  I := 0;
  REPEAT
    INC (I);
    READLN (INP, LINE);
    VLNPAR [I, 1] := ORD(LINE [13]) - ORD('0');
    VLNPAR [I, 2] := ORD(LINE [15]) - ORD('0');
    VLNSTR [I] := COPY (LINE, 17, LENGTH(LINE)-16);
  UNTIL EOF (INP) OR (I = MAXVLN);
  CURVLN := I;
  CLOSE (INP);
  writeln ('VL_NOUNS are ', CURVLN);
END;

BEGIN
  FILL_BASE_NOUNS;  
  VLN_ARRAY_INIT;
END;            { GLOBAL_INIT }

