                STG := NZPRT2;
              END
          ELSE IF IS_CENTURY (PART1) AND (PART2 = '')
            THEN  { like "21/^X^X^I " }
              BEGIN 
                CLS1 := 0; 
                CLS2 := 1;
                STG := NZPRT2;
              END
          ELSE IF (AMONG_NUMERALS(PART1, ERC) = BIG1)
            THEN
              IF (POS (BL, PART2) > 0)
                THEN WRITELN ('Error: Insert ', NOUN, ' to vlong nouns')
              ELSE
                BEGIN
                  NUM0 := SING;  GND0 := NEUT;
                  INITSUBCL (PART2, CLS2, VAL2, NUM0, GND0, ANM2);
                  IF ((CLS2 = 258) OR (CLS2 =228) OR (CLS2 =229) OR (CLS2 =233)) 
                    AND (ANM2 = NO)
                    THEN
                      IF (PART1 = '') OR (PART1 = '')
                        THEN CLS1 := 305
                        ELSE CLS1 := 289
                  ELSE IF ((CLS2 = 258) OR (CLS2 =227) OR (CLS2 =228) 
                    OR (CLS2 =229) OR (CLS2 =284) OR (CLS2 =233) 
                    OR (CLS2 =239) OR (CLS2 =259) OR (CLS2 =306) OR (CLS2 =307)) 
                    AND (ANM2 = YES)
                    THEN  
                      IF (PART1 = '') OR (PART1 = '')
                        THEN CLS1 := 208
                        ELSE CLS1 := 207;
