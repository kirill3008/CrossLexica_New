VAR
  TKN1, TKN2, PART1, PART2, LPART, RPART : STR115;
  PB, PH, PU, PI, PDA, PILI, PBHB, HBB, L, L1, L2 : INTEGER;
  ANCL, ANCL2, DEANCL, CLSX, CLSY : INTEGER;
