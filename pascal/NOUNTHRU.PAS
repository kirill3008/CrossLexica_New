{ Module for deterning inflection class for ANY noun group.
  Uses CLASTABL. Includes subprogram for nuneric group }
UNIT NOUNTHRU;

INTERFACE

USES DOS, CLASTABL;
	
CONST
  HY = '-';
  US = '_';
  MAXVLN = 16000; { Increased !!! }
  MAXBSN = 42000; { Increased !!! }
TYPE
  STR1 = STRING [1];
  STR20 = STRING [20];
  STR40 = STRING [40];
  STR115 = STRING [115];
  STAGE = (UNRZD, BASIC, VLONG, SINGLE, HYPHD, 
           DBLZR, ZPRT2, NZPRT2, CONJD);
  SCALE = (NOTNUM, UNUS, SMALL, BIG1, BIG2);
VAR
  VLNPAR : ARRAY [1..MAXVLN, 1..2] OF INTEGER;{ vlong noun numbs and ^gends }
  VLNSTR : ARRAY [1..MAXVLN] OF STR115;        { vlong noun strings }
  BSNCLS : ARRAY [1..MAXBSN, 1..2] OF INTEGER;{ basic noun classes }
  BSNPAR : ARRAY [1..MAXBSN, 1..2] OF INTEGER;{ basic noun numbs and gends }
  BSNSTR : ARRAY [1..MAXBSN] OF STR115;        { basic noun strings }
  CURVLN, BN_REACHED : LONGINT;

{ Initialization of both outer arrays }  
PROCEDURE GLOBAL_INIT;
        {============}

PROCEDURE PARSENOUN (NOUN0 : STR115;
         {=========} VAR NOUN : STR115;
                     VAR CLS1 : INTEGER;
                     VAR CLS2 : INTEGER;
                     VAR NUM0 : NUMBER;
                     VAR GND0 : GENDER;
                     VAR STG : STAGE);

