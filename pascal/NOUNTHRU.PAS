BEGIN           { MAIN }
{ Initialization of NOUN parameters }
  NOUN := NOUN0;
  CLS1 := 255;
  CLS2 := 255;
  NUM0 := UNKN;
  GND0 := NONE;
  STG := UNRZD;

  L := LENGTH (NOUN);
   
{ Start classification procedure }
  IF IN_BASIC_NOUNS (NOUN, CLS1, CLS2, NUM0, GND0)
    THEN STG := BASIC { (<= already classified NOUN }

  ELSE IF AMONG_TOO_LONG_N (NOUN, NUM0, GND0)
    THEN   { Very long NOUN }
      BEGIN
        CLS1 := 0; CLS2 := 0;
        STG := VLONG;
      END

  ELSE IF (NOUN = '  ')
    THEN   { Specific NOUN with changing POS }
      BEGIN
        CLS1 := 64; CLS2 := 0;
        NUM0 := SING; GND0 := FEM;
        STG := CONJD;
      END
 
  ELSE IF (NOUN[1] = '"') AND (NOUN[LENGTH(NOUN)] = '"')
(* Day time by ciphers and ':' within *) 
    OR ((LENGTH(NOUN) = 5) AND (NOUN[3] = ':')
      AND (('0' <= NOUN[1]) AND (NOUN[1] <= '2'))
      AND (('0' <= NOUN[2]) AND (NOUN[2] <= '9'))
      AND (('0' <= NOUN[4]) AND (NOUN[4] <= '5'))
      AND (('0' <= NOUN[5]) AND (NOUN[5] <= '9')))
    THEN 
      BEGIN
        CLS1 := 0; CLS2 := 255;
        NUM0 := SING; GND0 := NEUT;
        STG := SINGLE;
      END
(* Names of Soviet tnks *)
 
  ELSE IF ((LENGTH(NOUN) = 5) AND (COPY(NOUN,1,3) = '^-') 
    AND ('0' <= NOUN[4]) AND (NOUN[4] <= '9')
    AND ('0' <= NOUN[5]) AND (NOUN[5] <= '9'))
    OR (NOUN = '^^-2') OR (NOUN = '^^-1') OR (NOUN = '^^-2')
    THEN
      BEGIN
        CLS1 := 0; CLS2 := 255;
        NUM0 := SING; GND0 := MASC;
        STG := SINGLE;
      END
(* Hence another not very long NOUN *)
  ELSE    
    BEGIN
      PB := POS (BL, NOUN); { Blank }
      PH := POS (HY, NOUN); { Hyphen }
      PU := POS (US, NOUN); { UnderScore instead of hyphen }
      IF PB = 0       { No blanks in NOUN }
        THEN
begin
          IF (PH = 0) AND (PU = 0) { Single NOUN w/o hypen nor underscore }
            THEN
              BEGIN
                IF (L > 1) AND (POS (NOUN[L], '123456') > 0)
                  AND ('' <= NOUN[L-1]) AND (NOUN[L-1] <= '')
                  THEN WRITELN ('ERROR: Insert ', NOUN, ' to BASICS');

                IF (POS (NOUN[L], '0123456789') = 0)
                  AND IN_BASIC_NOUNS (NOUN+'1', CLSX, CLSY, NUMX, GNDX)
                  THEN WRITELN ('ERROR: Supply ', NOUN, ' with homonym number');

                IF (AMONG_NUMERALS(NOUN, ERC) = BIG1)
                  THEN
                    BEGIN
                      NUM0 := PLUR; GND0 := COMN; ANM1 := YES;
                      IF (NOUN = '') OR (NOUN = '')
                        THEN CLS1 := 208
                        ELSE CLS1 := 207;
                    END
                  ELSE INITSUBCL (NOUN, CLS1, VAL1, NUM0, GND0, ANM1);
                CLS2 := 255;
                STG := SINGLE;
              END
          ELSE IF (PU > 0)  { NOUN is pair with underscore between }
            THEN
              BEGIN
                TKN1 := COPY (NOUN, 1, PU-1);
                INITSUBCL (TKN1, CLS1, VAL1, NUM0, GND0, ANM1);
                TKN2 := COPY (NOUN, PU+1, L-PU);
                INITSUBCL (TKN2, CLS2, VAL2, NUM2, GND2, ANM2);

                               { Special pairs }
                IF (NOUN = '_') 
                  THEN GND0 := MASC;
               
                IF (NOUN = '_') 
                  OR (NOUN = '_') 
                  OR (NOUN = '_') 
                  OR (NOUN = '_') 
                  THEN GND0 := FEM;

                       { TKN1 danimates TKN2 }
                IF (TKN1 = '') AND (TKN2 = '')
                  THEN 
                    BEGIN 
                      CLS1 := 1; CLS2 := 60; 
                      NUM0 := SING; GND0 := MASC; 
                    END;

                       { TKN1 animates TKN2 }
                IF (ANM1 = YES) AND (ANM2 = NO)
                  AND CAN_BE_ANIMATED (CLS2, ANCL)
                  THEN CLS2 := ANCL;  { like - 
                                        or - 
                                        or -
                                        or - }
               
                       { TKN2 animates TKN1 }
                IF (ANM1 = NO) AND (ANM2 = YES)
                  AND CAN_BE_ANIMATED (CLS1, ANCL)
                  THEN CLS1 := ANCL;  { like -
                                        or -
                                        or - 
                                        or -
                                        or - 
                                        or - }

                        { TKN2 nearly repeats TKN1 }
                IF (NOUN = '_') OR (NOUN = '_')
                  OR (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 90; CLS2 := 90; 
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
                 IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 43; CLS2 := 43; 
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
                IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 37;   CLS2 := 37; 
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
                IF (NOUN = '_') OR (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 30;   CLS2 := 30; 
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
                IF (NOUN  = '_')
                  THEN
                    BEGIN
                      CLS1 := 163;  CLS2 := 163;
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
                IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 182;  CLS2 := 182; 
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
         
                     { One part corrects another }
                IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 86;   CLS2 := 86; 
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
                IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 90;   CLS2 := 41; 
                      NUM0 := PLUR; GND0 := COMN; 
                    END;
                IF (NOUN = 'C_')
                  THEN 
                    BEGIN 
                      CLS1 := 12;   CLS2 := 86; 
                      NUM0 := PLUR; GND0 := COMN; 
                   END;

                    { Sex-depending cases }
                IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 7;    CLS2 := 7; 
                      NUM0 := SING; GND0 := FEM; 
                    END;
                IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 8;    CLS2 := 8; 
                      NUM0 := SING; GND0 := FEM; 
                    END;
                IF (NOUN = '_')
                  THEN 
                    BEGIN 
                      CLS1 := 8;    CLS2 := 2; 
                      NUM0 := SING; GND0 := MASC; 
                    END;


                NOUN [PU] := HY; { Hyphen is restored }
                STG := HYPHD;
              END
   
          ELSE IF (PH > 0)  { NOUN contains hyphen }
            THEN
              BEGIN
                TKN1 := COPY (NOUN, 1, PH-1);
                TKN2 := COPY (NOUN, PH+1, L-PH);
                IF DECL_BEFORE_HYPH (TKN1)
                  THEN
                    BEGIN
                      INITSUBCL (TKN1, CLS1, VAL1, NUM0, GND0, ANM1);
                      INITSUBCL (TKN2, CLS2, VAL2, NUM2, GND2, ANM2);
                      IF (ANM1 = NO) AND (ANM2 = YES) 
                        AND CAN_BE_DEANIMATED (CLS2, DEANCL)
                        THEN CLS2 := DEANCL; { like - }
                      STG := HYPHD;
                    END
                  ELSE
                    CASE STOP_HYPHENING (TKN1) OF
                      0 :
                        BEGIN
                          INITSUBCL (NOUN, CLS1, VAL1, NUM0, GND0, ANM1);
                          CLS2 := 255;
                          STG := SINGLE;       { like - }
                        END;
                      1 :
                        BEGIN
                          INITSUBCL (TKN2, CLS1, VAL2, NUM0, GND0, ANM2);
                          CLS2 := 255;
                          IF CAN_BE_ANIMATED (CLS1, ANCL)
                            THEN CLS1 := ANCL;     { like - }
                          STG := SINGLE;
                        END;
                      2:
                        BEGIN
                          INITSUBCL (TKN2, CLS1, VAL2, NUM0, GND0, ANM2);
                          CLS2 := 255;
                          STG := SINGLE;        { like - }
                        END;
                    END; { CASE }
              END
end
      ELSE       { There is a blank or more in NOUN }
        BEGIN
          PBHB := POS (' - ', NOUN); 
          PI := POS('  ', NOUN);
        
          TKN1 := COPY (NOUN, 1, PB-1);
          TKN2 := COPY (NOUN, PB+1, L-PB);

                { TKN1 is anmong non-zero classes }
          IF AMONG_INITS(TKN1) 
            AND (COPY(NOUN,PB,3) <> '  ')
            AND (COPY(NOUN,PB,5) <> '  ')
            AND (COPY(NOUN,PB,4) <> '  ')
            THEN
              BEGIN
                INITSUBCL (TKN1, CLS1, VAL1, NMB1, GND1, ANM1);
             
                IF (NOUN = '  ') 
                  OR (NOUN = ' ^')
                  OR (NOUN = ' ') 
                  OR (NOUN = ' ') 
                  OR (NOUN = ' ') 
                  OR (NOUN = ' ') 
                  OR (NOUN = ' ') 
                  OR (TKN1 = '') 
                  OR ((TKN1 = '') 
                    AND ((COPY(NOUN,PB,4) = '  ') OR (NOUN[PB+1] = '^'))
                      OR (TKN2 = '007') 
                      OR (TKN2 = '')OR (TKN2 = ''))
                  THEN CLS1 := 2;

                IF ((TKN1 = '') 
                  AND ((COPY(NOUN,PB,4) = '  ') OR (NOUN[PB+1] = '^'))
                    OR (TKN2 = '') OR (TKN2 = ''))
                  THEN CLS1 := 87;
                      
                IF (TKN1 = '') AND (ORG_NAME_GEN(TKN2) 
                  OR (TKN2 = ' ')
                  OR (TKN2 = ' ')
                  OR (TKN2 = '   '))
                  THEN CLS1 := 146;

                IF (NOUN = '  ') 
                  OR (NOUN = '  ') 
                  THEN CLS1 := 2;
                   
                NUM0 := NMB1; GND0 := GND1;

                IF (TKN1 = '') AND NOT TEXT_GEN (TKN2)
                  THEN GND0 := MASC;
                              
                CLS2 := 0;
                EXIT;
              END;

      {  as initial word, several options }
          IF (TKN1 = '') 
            THEN
              BEGIN
                INITSUBCL (TKN2, CLS2, VAL2, NUM2, GND2, ANM2);
              
                IF (TKN2 = '') OR (TKN2 = '  ') 
                  OR (TKN2 = ' ') OR (TKN2 = ' ') 
                  OR (TKN2 = '') OR (TKN2 = '') 
                  OR (TKN2 = ' ') OR (TKN2 = ' ') 
                  OR (TKN2 = ' ') OR (TKN2 = '') 
                  OR (TKN2 = '  ') OR (TKN2 = '')
                  OR (TKN2 = '   ') 
                  OR (TKN2 = '   ')
                  THEN
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN;
                      CLS1 := 163;    CLS2 := 0; 
                      ANM1 := YES;    STG := ZPRT2;
                    END
                ELSE IF (TKN2 = ' ') 
                  THEN
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN;
                      CLS1 := 163;    CLS2 := 61; 
                      ANM1 := YES;    STG := NZPRT2;
                    END
                ELSE IF (TKN2 = '')
                  THEN    
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN; 
                      CLS1 := 163;    CLS2 := 87;
                      ANM1 := YES;    STG := NZPRT2;
                    END  
                ELSE IF (CLS2 <> 0) AND (NUM2 = PLUR) 
                  AND (GND2 = COMN) AND (ANM2 = YES) 
                  THEN    
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN; 
                      CLS1 := 163;    
                      ANM1 := YES;    STG := NZPRT2;
                    END  
                ELSE IF (AMONG_NUMERALS(TKN2, ERC) = BIG1)
                  THEN
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN; 
                      CLS1 := 163;
                      IF (TKN2 = '')
                        THEN CLS2 := 208
                        ELSE CLS2 := 207;
                      ANM1 := YES;   STG := NZPRT2;
                    END
                ELSE IF (TKN2 = '')
                  THEN    
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN; 
                      CLS1 := 169;    CLS2 := 287;
                      ANM1 := NO;     STG := NZPRT2; 
                    END  
                ELSE IF (CLS2 <> 0) AND (NUM2 = PLUR) 
                  AND (GND2 = COMN) AND (ANM2 = NO) 
                  THEN    
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN; 
                      CLS1 := 163;    
                      ANM1 := NO;    STG := NZPRT2;
                    END
                ELSE IF (TKN2 = ' ')
                  OR (TKN2 = ' ') 
                  OR (TKN2 = '') 
                  OR (TKN2 = ' ')
                  THEN 
                    BEGIN 
                      NUM0 := SING;   GND0 := NEUT; 
                      CLS1 := 20;     CLS2 := 0; 
                      ANM1 := NO;     STG := ZPRT2;
                    END
                ELSE IF (CLS2 <> 0) AND (NUM2 = SING) 
                  AND (GND2 = NEUT) 
                  THEN    
                    BEGIN 
                      NUM0 := SING;   GND0 := NEUT; 
                      CLS1 := 20;    
                      ANM1 := NO;    STG := NZPRT2;
                    END  
                ELSE WRITELN ('Error: ', TKN2, ' after  seams unusual');
                EXIT;
              END;    
                     
                { Two actor names separated with ' - ' }
          IF (PBHB > 0) AND (NOUN[1] = '^') AND (NOUN[PBHB+3] = '^')
            THEN          
              BEGIN 
                PART1 := COPY (NOUN, 1, PBHB-1);
                PART2 := COPY (NOUN, PBHB+3, L-PBHB-2);
                INITSUBCL (PART1, CLS1, VAL1, NUM1, GND1, ANM1);
                INITSUBCL (PART2, CLS2, VAL2, NUM0, GND0, ANM2);
                NUM0 := PLUR; GND0 := COMN;
                EXIT;
              END;

                { Two actor names separated with '  ' }
          IF (PI > 0) AND (NOUN[1] = '^') AND (NOUN[PI+3] = '^')
            THEN          
              BEGIN 
                PART1 := COPY (NOUN, 1, PI-1);
                PART2 := COPY (NOUN, PI+3, L-PI-2);
                INITSUBCL (PART1, CLS1, VAL1, NUM1, GND1, ANM1);
                INITSUBCL (PART2, CLS2, VAL2, NUM0, GND0, ANM2);
                NUM0 := PLUR; GND0 := COMN;
                EXIT;
              END;
       
                { Feminine names pairs with or w/o agreement }
          IF (FEM_ADRESSING(TKN1) OR FEMIN_NOMEN(TKN1)) 
            AND (TKN2[1] = '^') AND (POS('^',TKN2) = 0)
            THEN
              BEGIN
                INITSUBCL (TKN1, CLS1, VAL1, NMB1, GND1, ANM1);
                IF (GND1 <> FEM) THEN CLS1 := 0;
                INITSUBCL (TKN2, CLS2, VAL2, NMB2, GND2, ANM2);
                IF (GND2 <> FEM) THEN CLS2 := 0;
                NUM0 := SING; GND0 := FEM;
                EXIT; 
              END;
 
                 { Special cases of desagreement for masculine names }
          IF (NOUN = '^ ^') 
            THEN 
              BEGIN 
                CLS1 := 74; CLS2 := 2;
                NUM0 := SING; GND0 := MASC;  
                STG := NZPRT2;
                EXIT;
              END;
          IF (NOUN = '^ ^') 
            OR (NOUN = ' ^') 
            THEN 
              BEGIN 
                CLS1 := 8; CLS2 := 2;
                NUM0 := SING; GND0 := MASC;  
                STG := NZPRT2;
                EXIT;
              END;
          IF (NOUN = '^ ^') 
            THEN 
              BEGIN 
                CLS1 := 0; CLS2 := 4;
                NUM0 := SING; GND0 := MASC;  
                STG := NZPRT2;
                EXIT;
              END;
          IF (NOUN = '^ ^') 
            THEN 
              BEGIN 
                CLS1 := 0; CLS2 := 60;
                NUM0 := SING; GND0 := MASC;  
                STG := NZPRT2;
                EXIT;
              END;
          IF (NOUN = '^ ^') 
            THEN 
              BEGIN 
                CLS1 := 2; CLS2 := 8;
                NUM0 := SING; GND0 := MASC;  
                STG := NZPRT2;
                EXIT;
              END;
          IF (NOUN = '^ ^') 
            THEN 
              BEGIN 
                CLS1 := 6; CLS2 := 6;
                NUM0 := SING; GND0 := MASC;  
                STG := NZPRT2;
                EXIT;
              END;


                 { If there is a conjunction in NOUN? }
          PDA := POS('  ', NOUN);
          PILI := POS('  ', NOUN);
          PBHB := POS(' - ', NOUN);
          HBB := POS('- ', NOUN);

                 { PART1 and PART2 are separated by the 1st blank }
          IF (PB > 0) {AND (PBHB = 0) }
            THEN          
              BEGIN 
                L := LENGTH (NOUN);
                L1 := PB - 1;
                L2 := L - PB;
                PART1 := COPY (NOUN, 1, L1);
                PART2 := COPY (NOUN, PB+1, L2);
                INITSUBCL (PART1, CLS1, VAL1, NUM1, GND1, ANM1);
                INITSUBCL (PART2, CLS2, VAL2, NUM0, GND0, ANM2);
              END;

          STG := CONJD;
  
          IF (IS_DAY_NUM (PART1) AND IS_MONTH_NAME (PART2))
              { Indeclinable dates like 11  }
            OR DBL_ZERO_NEUT (NOUN) { Neutral pairs like   }
            THEN          
              BEGIN 
                CLS1 := 0; CLS2 := 0;
                NUM0 := SING; GND0 := NEUT; 
                STG := DBLZR;
               END
          ELSE IF DBL_ZERO_MASC (NOUN)      
            THEN   { Indeclinable masculine pairs }
              BEGIN 
                CLS1 := 0; CLS2 := 0;
                NUM0 := SING; GND0 := MASC; 
                STG := DBLZR;
               END
          ELSE IF DBL_ZERO_FEM (NOUN)      
            THEN   { Indeclinable feminine pairs }
              BEGIN 
                CLS1 := 0; CLS2 := 0;
                NUM0 := SING; GND0 := FEM; 
                STG := DBLZR;
               END
          ELSE IF DBL_ZERO_PLUR (NOUN)
            THEN   { Indeclinable plural pairs }
              BEGIN 
                CLS1 := 0; CLS2 := 0;
                NUM0 := PLUR; GND0 := COMN; 
                STG := DBLZR;
               END

            { === Cases of zero classified PART1 === }

          ELSE IF (POS(' ', NOUN) = 1) OR ZERO_INIT (NOUN)
            THEN
              BEGIN
                CLS1 := 0;
                INITSUBCL (PART2, CLS2, VAL2, NUM0, GND0, ANM2);
                IF ( (PART2 = '') OR (PART2 = '') 
                  OR (PART2 = '') OR (PART2 = '') 
                  OR (PART2 = '') OR (PART2 = '') )
                  AND CAN_BE_ANIMATED (CLS2, ANCL)
                  THEN CLS2 := ANCL;
                IF (PART1 = '^') OR (PART1 = '') 
                  {  ⢮/⥩⢮ }
                  THEN BEGIN NUM0 := SING; GND0 := MASC; END;
                STG := NZPRT2;
              END

            { === Cases of non-zero classified PART2 === }

      {  as initial word, several options }
          ELSE IF (PART1 = '') 
            THEN
              BEGIN
                INITSUBCL (PART2, CLS2, VAL2, NUM2, GND2, ANM2);
              
                IF (PART2 = '') OR (PART2 = '') 
                  OR (PART2 = ' ') OR (PART2 = ' ') 
                  OR (PART2 = ' ') OR (PART2 = '') 
                  OR (PART2 = '') OR (PART2 = '')
                  THEN
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN;
                      CLS1 := 163;    CLS2 := 0; 
                      ANM1 := YES;    STG := ZPRT2;
                    END
                ELSE IF (PART2 = ' ') 
                  THEN
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN;
                      CLS1 := 163;    CLS2 := 61; 
                      ANM1 := YES;    STG := NZPRT2;
                    END
                ELSE IF (PART2 = '')
                  THEN    
                    BEGIN 
                      NUM0 := PLUR;   GND0 := COMN; 
